              </xsd:element>
              <xsd:element name="MinumumToughness">
                <xsd:complexType>
                  <xsd:attribute name="value" type="xsd:unsignedByte" use="required" />
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="MinumumWounds">
                <xsd:complexType>
                  <xsd:attribute name="value" type="xsd:unsignedByte" use="required" />
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="MinumumAttacks">
                <xsd:complexType>
                  <xsd:attribute name="value" type="xsd:unsignedByte" use="required" />
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="MinumumLeadership">
                <xsd:complexType>
                  <xsd:attribute name="value" type="xsd:unsignedByte" use="required" />
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="MinumumSave">
                <xsd:complexType>
                  <xsd:attribute name="value" type="xsd:unsignedByte" use="required" />
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xs:schema>